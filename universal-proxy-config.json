{
    "platforms": {
        "xname": {
            "base_url": "https://gateway.xname.app",
            "api_key": {
                "location": "header",
                "param_name": "x-api-key"
            },
            "endpoints": {
                "base": {
                    "path": "/xhs/level/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "talentprotocol": {
            "base_url": "https://api.talentprotocol.com",
            "api_key": {
                "location": "header",
                "param_name": "X-API-KEY",
                "prefix": "Bearer"
            },
            "endpoints": {
                "base": {
                    "path": "/api/v1/passports/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        }
    }
}
