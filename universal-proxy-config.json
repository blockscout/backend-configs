{
    "platforms": {
        "blockscoutbadges": {
            "base_url": "https://badges.blockscout.com",
            "endpoints": {
                "base": {
                    "path": "/api/badges/:address/counter",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "xname": {
            "base_url": "https://gateway.xname.app",
            "api_key": {
                "location": "header",
                "param_name": "x-api-key"
            },
            "endpoints": {
                "base": {
                    "path": "/xhs/level/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "talentprotocol": {
            "base_url": "https://api.talentprotocol.com",
            "api_key": {
                "location": "header",
                "param_name": "X-API-KEY"                
            },
            "endpoints": {
                "base": {
                    "path": "/score",
                    "method": "GET",
                    "params": [
                        {
                            "location": "query",
                            "name": "id",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "efp": {
            "base_url": "https://api.ethfollow.xyz",            
            "endpoints": {
                "base": {
                    "path": "/api/v1/users/:address/stats",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",                            
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "webacy":  {
            "base_url": "https://api.webacy.com",
            "api_key": {
                "location": "header",
                "param_name": "x-api-key"
            },
            "endpoints": {
                "base": {
                    "path": "/addresses/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "humanpassport":  {
            "base_url": "https://api.passport.xyz",
            "api_key": {
                "location": "header",
                "param_name": "X-API-KEY"
            },
            "endpoints": {
                "base": {
                    "path": "/v2/stamps/10791/score/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "trustblock": {
            "base_url": "https://api.trustblock.run",
            "api_key": {
                "location": "header",
                "param_name": "Authorization",
                "prefix": "Bearer"
            },
            "endpoints": {
                "base": {
                    "path": "/v1/project/security",
                    "method": "GET",
                    "params": [
                        {
                            "location": "query",
                            "type": "address",
                            "name": "address"
                        },                  
                        {
                            "location": "query",
                            "type": "chain_id_dependent",
                            "name": "chain",
                            "mapping": {
                                "1": "ethereum",
                                "10": "optimism",
                                "137": "polygon",
                                "324": "zksync",
                                "8453": "base",
                                "42161": "arbitrum",
                                "534352": "scroll"
                            }
                        }
                    ]
                }
            }
        },
        "bankless": {
            "base_url": "https://api.bankless.com",
            "api_key": {
                "location": "header",
                "param_name": "X-BANKLESS-TOKEN"
            },
            "endpoints": {
                "base": {
                    "path": "/claimables/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "deepdao": {
            "base_url": "https://api.deepdao.io",
            "api_key": {
                "location": "header",
                "param_name": "x-api-key"
            },
            "endpoints": {
                "base": {
                    "path": "/v0.1/people/dao_gscore/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "smartmuv": {
            "base_url": "https://api.smartmuv.app",
            "endpoints": {
                "base": {
                    "path": "/contract/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "etherscore": {
            "base_url": "https://beta.etherscore.network",
            "api_key": {
                "location": "header",
                "param_name": "x-api-key"
            },
            "endpoints": {
                "base": {
                    "path": "/public-api/v1/get_card/:address",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        },
        "bubblemaps": {
            "base_url": "https://api.bubblemaps.io",
            "api_key": {
                "location": "header",
                "param_name": "X-ApiKey"
            },
            "endpoints": {
                "base": {
                    "path": "/maps/:chain/:address?return_decentralization_score=true",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        },
                        {
                            "location": "path",
                            "type": "chain_id_dependent",
                            "name": "chain",
                            "mapping": {
                                "1": "eth",                              
                                "137": "polygon",                                
                                "8453": "base"                                
                            }
                        }
                    ]
                }
            }
        },
        "goplus": {
            "base_url": "https://api.gopluslabs.io",
            "api_key": {
                "location": "header",
                "param_name": "x-api-key"
            },
            "endpoints": {
                "base": {
                    "path": "/api/v1/token_security/:chain_id",
                    "method": "GET",
                    "params": [                
                        {
                            "location": "path",
                            "type": "chain_id"                           
                        },
                        {
                            "location": "query",
                            "type": "address",
                            "name": "contract_addresses"
                        }
                    ]
                }
            }
        },
        "gitpoap": {
            "base_url": "https://public-api.gitpoap.io",
            "endpoints": {
                "base": {
                    "path": "/v1/address/:address/gitpoaps",
                    "method": "GET",
                    "params": [
                        {
                            "location": "path",
                            "type": "address"
                        }
                    ]
                }
            }
        }
    }
}
